# the url of the graphql backend server
APOLLO_SERVER_URL=http://localhost:4000/graphql
# the secret generated by npx auth secret 
AUTH_SECRET=your-auth-secret
# should be the same as the one in apps/server/.env
AUTH_JWT_SECRET=your-auth-jwt-secret
# the username for the dummy user
AUTH_DUMMY_USERNAME=your-dummy-username
# the password for the dummy user
AUTH_DUMMY_PASSWORD=your-dummy-password
# must be the same as the issuer in apps/server/.env
AUTH_ISSUER=your-auth-issuer
# must be the same as the audience in apps/server/.env
AUTH_AUDIENCE=your-auth-audience
